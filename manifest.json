{
    "manifest_version": 2,
    "version": "0.1",
    "background": {
        "persistent": true,
        "scripts": ["events.js"]
    },
    "content_scripts": [{
        "matches": ["*://t-square.gatech.edu/portal/tool/*/studentView.jsf"],
        "js": ["lib/jquery-2.0.3.js", "gradebook.js"],
        "all_frames": true
    }],
    "name": "T3 Gradebook",
    "icons": {
        "16": "img/icon-16.png",
        "48": "img/icon-48.png"
    },
    "browser_action": {
        "default_title": "TÂ³ Gradebook",
        "default_popup": "popup.html",
        "default_icon": {
            "19": "img/icon-19.png",
            "38": "img/icon-38.png"
        }
    },
    "permissions": [
        "webRequest",
        "*://t-square.gatech.edu/*"
    ]
}
